From 7ade474276f5b6db619fe36fed35c1cbfc7de046 Mon Sep 17 00:00:00 2001
From: Mike Labriola <labriola@mail.neo>
Date: Fri, 20 Aug 2010 21:20:27 +0000
Subject: [PATCH] set NATIVE_SYSTEM_HEADER_DIR

---
 gcc/Makefile.in |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/gcc/Makefile.in b/gcc/Makefile.in
index ae2416b..69338aa 100644
--- a/gcc/Makefile.in
+++ b/gcc/Makefile.in
@@ -438,7 +438,7 @@ LINKER_PLUGIN_API_H = $(srcdir)/../include/plugin-api.h
 LTO_SYMTAB_H = $(srcdir)/../include/lto-symtab.h
 
 # Default native SYSTEM_HEADER_DIR, to be overridden by targets.
-NATIVE_SYSTEM_HEADER_DIR = /usr/include
+NATIVE_SYSTEM_HEADER_DIR = /bootstrap/include
 # Default cross SYSTEM_HEADER_DIR, to be overridden by targets.
 CROSS_SYSTEM_HEADER_DIR = @CROSS_SYSTEM_HEADER_DIR@
 
